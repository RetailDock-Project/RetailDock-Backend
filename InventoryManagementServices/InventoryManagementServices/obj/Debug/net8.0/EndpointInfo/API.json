            "enum": [
              "CGST_SGST",
              "IGST",
              "UGST_CGST"
            ],
            "type": "string"
          },
          "purchaseItems": {
            "minItems": 1,
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/PurchaseItemDto"
            }
          }
        },
        "additionalProperties": false
      },
